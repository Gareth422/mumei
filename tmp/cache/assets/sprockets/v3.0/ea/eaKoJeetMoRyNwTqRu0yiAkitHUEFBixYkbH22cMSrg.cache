  var PostHandle;

  PostHandle = (function() {
    function PostHandle(container) {
      var $container, filter, i, len, ref;
      this.container = container;
      $container = $(this.container);
      ref = this.constructor.filters;
      for (i = 0, len = ref.length; i < len; i++) {
